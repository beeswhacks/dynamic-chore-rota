<% include _header %>

<% tasks.forEach(function(task) { %>
    <div class="cleanerContainer"> 
        <div class="cleanerName">
            <%= task.cleaner%>
        </div>
        <div class="cleanerContent">
            <div>
                <p class="cleaningTasks"> <%= task.room%>, <%= task.level%> clean </p>
            </div>
        </div>
    </div> 
<% }) %>

<div id="footer">
    <p id="footerPara"></p>
</div>

<% include _footer %>